package lexer;

import java.io.*;

public class LexerMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}
	
	public String[] readFile() throws IOException {
		String[] explodedString = null;
		File = new File("C:\\Users\\MUAH\\Documents\\compilers\\code.txt");
		try {
			in = new FileInputStream("C:\\Users\\MUAH\\Documents\\compilers\\code.txt");
			
			int c;
			while ((c = in.read()) != -1) {
				explodedString = c.split("\\s+");
				
			}
		}
	}

}
