package parser.objects;

import java.util.ArrayList;

public class CharList {
	private static int postIndex;
	private static Char charVar;
	private static CharList charList;
	private static Space space;
	public CharList() {
		charVar = new Char();
		charList = new CharList();
		space = new Space();
	}
	
	public static boolean validate(String input, int currIndex) {
		if (currIndex == -1) {
			currIndex = 0;
		}
		
		String[] result = input.split("");

		if (charVar.validate(result, currIndex)) {
			if (charList.validate(result, charVar.getPostIndex()))
		}
	}
}
